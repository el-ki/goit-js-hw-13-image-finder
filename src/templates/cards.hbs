<ul class="gallery">
    {{#each this}}
    <div class="photo-card">
        <a href="{{largeImageURL}}"=> <img class="image" src="{{webformatURL}}" alt="{{tags}}" height="230" /></a>
        <div class="stats">
            <p class="stats-item">
                <i class="material-icons">thumb_up</i>
                {{likes}}
            </p>
            <p class="stats-item">
                <i class="material-icons">visibility</i>
                {{views}}
            </p>
            <p class="stats-item">
                <i class="material-icons">comment</i>
                {{comments}}
            </p>
            <p class="stats-item">
                <i class="material-icons">cloud_download</i>
                {{downloads}}
            </p>
        </div>
    </div>
    {{/each}}
</ul>